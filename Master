#include <stdio.h>
int main ()

{
int i, j, m1[3][3], m2[3][3], sum[3][3];

printf("Enter The Values Of Matrix 1 wrt Row Major \n");

for(i=0;i<3;i++)
   {
    for(j=0;j<3;j++)
       {
        scanf("%d",&m1[i][j]);
       }
   }

printf("Enter The Values Of Matrix 2 wrt Row Major \n");

for(i=0;i<3;i++)
   {
    for(j=0;j<3;j++)
       {
        scanf("%d",&m2[i][j]);
       }
   }

sum[i][j] = m1[i][j] + m2[i][j];

printf("The Sum is \n");

for(i=0;i<3;i++)
   {
    for(j=0;j<3;j++)
       {
        printf("%d  ",sum[i][j] = m1[i][j] + m2[i][j]);
       }
        printf("\n");  
   }
return 0;
}

